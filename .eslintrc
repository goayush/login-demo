{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"plugin:prettier/recommended",
		"prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"import/export": 0
	},
	"settings": {
		"import/resolver": {
			"alias": {
				"map": [
					["@core", "./src/components/core"],
					["@page", "./src/components/page"],
					["@part", "./src/components/part"],
					["@config", "./src/config"],
					["@context", "./src/context"],
					["@theme", "./src/theme"],
					["@helper", "./src/helper"]
				],
				"extensions": [".js"]
			}
		}
	}
}
